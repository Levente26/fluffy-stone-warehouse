{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "price": {
      "pluginOptions": {},
      "type": "decimal",
      "required": true
    },
    "quantity": {
      "pluginOptions": {},
      "type": "integer",
      "required": true
    },
    "warehouse": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::warehouse.warehouse",
      "inversedBy": "products"
    },
    "category": {
      "pluginOptions": {},
      "type": "enumeration",
      "enum": [
        "electronics",
        "jewelery",
        "men's clothing",
        "women's clothing"
      ],
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "rating": {
      "pluginOptions": {},
      "type": "decimal"
    },
    "numberOfRatings": {
      "pluginOptions": {},
      "type": "integer"
    }
  }
}
