{
  "kind": "collectionType",
  "collectionName": "packages",
  "info": {
    "singularName": "package",
    "pluralName": "packages",
    "displayName": "Package"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "warehouse": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::warehouse.warehouse",
      "inversedBy": "packages"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "rate": {
      "required": true,
      "type": "decimal"
    },
    "countRates": {
      "type": "integer",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "category": {
      "type": "string",
      "required": true
    }
  }
}
